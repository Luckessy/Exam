              <columns>
                <TableColumn fx:id="TabID" prefWidth="38.0" text="ID" />
                <TableColumn fx:id="TabFIO" prefWidth="116.0" text="FIO" />
                  <TableColumn fx:id="TabZP" prefWidth="122.0" text="ZP" />
